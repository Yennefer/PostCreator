<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/thumbnail_stars"/>
    <item android:left="@dimen/thumb_padding"
          android:right="@dimen/thumb_padding"
          android:top="@dimen/thumb_padding"
          android:bottom="@dimen/thumb_padding">
        <shape android:shape="rectangle">
            <corners android:radius = "@dimen/thumb_corner_big_radius" />
            <stroke android:color="?attr/colorPrimary"
                    android:width="@dimen/thumb_frame_width"/>
        </shape>
    </item>
</layer-list>